<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:paddingBottom="@dimen/activity_vertical_margin"
android:paddingLeft="@dimen/activity_horizontal_margin"
android:paddingRight="@dimen/activity_horizontal_margin"
android:background="@color/accent_material_light"
android:paddingTop="@dimen/activity_vertical_margin"
app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".MainActivity"
>
<ImageView
    android:layout_width="70dp"
    android:layout_height="100dp"
    android:id="@+id/iv_home"
    android:textAlignment="center"
    android:layout_marginLeft="30dp"
    android:src="@drawable/home1"/>
<ImageView
    android:layout_width="70dp"
    android:layout_height="wrap_content"
    android:layout_marginTop="140dp"
    android:id="@+id/iv_gallery"
    android:layout_marginLeft="30dp"
    android:src="@drawable/gallery1"/>
<ImageView
    android:layout_width="70dp"
    android:layout_height="70dp"
    android:id="@+id/iv_messages"
    android:layout_marginTop="280dp"
    android:layout_marginLeft="30dp"
    android:src="@android:drawable/stat_notify_chat"/>
<ImageView
    android:layout_width="70dp"
    android:layout_height="wrap_content"
    android:id="@+id/iv_friends"
    android:layout_marginLeft="240dp"/>
<ImageView
    android:layout_width="70dp"
    android:layout_height="wrap_content"
    android:id="@+id/iv_notifications"
    android:layout_marginTop="260dp"
    android:layout_marginLeft="240dp"
    />
<ImageView
    android:layout_width="70dp"
    android:layout_height="wrap_content"
    android:id="@+id/iv_stories"
    android:layout_marginLeft="240dp"
    android:layout_marginTop="140dp"
    />
<ImageView
    android:layout_width="70dp"
    android:layout_height="70dp"
    android:id="@+id/iv_familytree"
    android:layout_marginLeft="10dp"
    android:layout_marginTop="418dp"
    android:src="@drawable/abc_ic_voice_search_api_mtrl_alpha" />
<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/iv_findmatch"
    android:layout_marginTop="420dp"
    android:layout_marginLeft="140dp"
    android:src="@drawable/abc_ic_search_api_mtrl_alpha" />
<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/iv_groups"
    android:layout_marginTop="420dp"
    android:layout_marginLeft="260dp"/>
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@+id/iv_home"
    android:layout_marginLeft="30dp"
    android:textColor="#fde1ab"
    android:textStyle="bold"
    android:textSize="26dp"
    android:text="Home"/>
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="240dp"
    android:layout_marginLeft="30dp"
    android:textColor="#fde1ab"
    android:textStyle="bold"
    android:text="Gallery"
    android:textSize="25dp"/>
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@+id/iv_friends"
    android:layout_marginLeft="240dp"
    android:textColor="#fde1ab"
    android:textStyle="bold"
    android:text="Friends"
    android:textSize="25dp"/>
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="240dp"
    android:layout_marginLeft="240dp"
    android:textColor="#fde1ab"
    android:textStyle="bold"
    android:text="Stories"
    android:textSize="25dp"/>
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@+id/iv_notifications"
    android:layout_marginLeft="200dp"
    android:textColor="#fde1ab"
    android:textStyle="bold"
    android:text="Notifications"
    android:textSize="25dp"/>
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="360dp"
    android:layout_marginLeft="30dp"
    android:textColor="#fde1ab"
    android:textStyle="bold"
    android:text="Messages"
    android:textSize="25dp"/>

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Audio"
    android:textStyle="bold"
    android:textSize="25dp"
    android:layout_marginLeft="30dp"
    android:layout_below="@+id/iv_familytree"
    android:textColor="#fde1ab"/>
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textSize="20dp"
    android:textStyle="bold"
    android:layout_marginLeft="130dp"
    android:textColor="#fde1ab"
    android:layout_below="@+id/iv_findmatch"
    android:text="Find match"/>
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginLeft="260dp"
    android:textSize="20dp"
    android:textStyle="bold"
    android:text="Groups"
    android:textColor="#fde1ab"
    android:layout_below="@+id/iv_groups"/>
</RelativeLayout>
